{
  "name": "recorder-js",
  "version": "1.0.7",
  "description": "Record audio in your browser",
  "main": "index.js",
  "scripts": {
    "prepublish": "rm -rf lib/ && ./node_modules/.bin/babel src/ -d lib/",
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "dev:serve": "cd example && ../node_modules/.bin/webpack-dev-server --open --port 5000 --host localhost"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "^6.24.1",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.22.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "^2.4.2",
    "yargs": "6.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ijsnow/studiojs.git"
  },
  "author": "Isaac Snow",
  "license": "MIT",
  "dependencies": {
    "inline-worker": "^1.1.0"
  }
}
